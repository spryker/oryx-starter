{
  "name": "fes-storefront",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:sw & npm run dev:client",
    "dev:client": "vite -c vite.config.ts",
    "dev:sw": "vite build --config vite.config.sw.ts --watch",
    "build": "npm run build:sw && npm run build:client",
    "build:sw": "rm -rf dev-dist/sw && vite build -c vite.config.sw.ts",
    "build:client": "vite build"
  },
  "dependencies": {
    "@spryker-oryx/presets": "latest",
    "@spryker-oryx/themes": "latest",
    "vite-plugin-pwa": "^0.16.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "vite": "^4.0.0",
    "vite-plugin-static-copy": "^0.13.0"
  }
}
